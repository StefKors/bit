{
  "name": "@bit/client",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "start": "bun run server.ts",
    "lint": "bun run lint:tsc && bun run lint:eslint",
    "lint:tsc": "tsc -b --noEmit",
    "lint:eslint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "oxfmt --write .",
    "format:check": "oxfmt --check .",
    "instant:push:schema": "INSTANT_SCHEMA_FILE_PATH=src/instant.schema.ts npx instant-cli push schema --yes",
    "instant:push:perms": "cp src/instant.perms.ts instant.perms.ts && npx instant-cli push perms --yes && rm instant.perms.ts",
    "instant:push": "bun run instant:push:schema && bun run instant:push:perms",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@base-ui/react": "^1.2.0",
    "@instantdb/admin": "^0.22.142",
    "@instantdb/react": "^0.22.142",
    "@octokit/webhooks-types": "^7.6.1",
    "@pierre/diffs": "^1.0.11",
    "@primer/octicons-react": "^19.22.0",
    "@shikijs/markdown-it": "^3.22.0",
    "@tanstack/react-devtools": "^0.9.6",
    "@tanstack/react-router": "^1.162.4",
    "@tanstack/react-router-devtools": "^1.162.4",
    "@tanstack/react-router-ssr-query": "^1.162.4",
    "@tanstack/react-start": "^1.162.4",
    "cmdk": "^1.1.1",
    "jose": "^6.1.3",
    "js-cookie": "^3.0.5",
    "markdown-it-async": "^2.2.0",
    "nitro": "^3.0.1-alpha.2",
    "octokit": "^5.0.5",
    "oxfmt": "^0.35.0",
    "react": "^19.2.4",
    "react-diff-viewer-continued": "^4.1.2",
    "react-dom": "^19.2.4",
    "shiki": "^3.22.0",
    "sst": "3.19.3",
    "vite-tsconfig-paths": "^6.1.1",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@hono/node-server": "^1.19.9",
    "@tanstack/devtools-vite": "^0.5.1",
    "@tanstack/router-plugin": "^1.162.4",
    "@types/bun": "^1.3.9",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^25.3.0",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.4",
    "eslint": "^10.0.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.5.1",
    "globals": "^17.3.0",
    "hono": "^4.12.2",
    "tsr": "^1.3.4",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.0",
    "vinxi": "^0.5.11",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  }
}
