{
  "name": "@bit/client",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "start": "bun run server.ts",
    "lint": "bun run lint:tsc && bun run lint:eslint",
    "lint:tsc": "tsc -b --noEmit",
    "lint:eslint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "oxfmt --write .",
    "format:check": "oxfmt --check .",
    "instant:push:schema": "INSTANT_SCHEMA_FILE_PATH=src/instant.schema.ts npx instant-cli push schema --yes",
    "instant:push:perms": "cp src/instant.perms.ts instant.perms.ts && npx instant-cli push perms --yes && rm instant.perms.ts",
    "instant:push": "bun run instant:push:schema && bun run instant:push:perms",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@base-ui/react": "^1.0.0",
    "@instantdb/admin": "^0.22.96",
    "@instantdb/react": "^0.22.96",
    "@octokit/webhooks-types": "^7.6.1",
    "@pierre/diffs": "^1.0.0",
    "@primer/octicons-react": "^19.21.1",
    "@shikijs/markdown-it": "^3.20.0",
    "@tanstack/react-devtools": "^0.9.0",
    "@tanstack/react-router": "^1.144.0",
    "@tanstack/react-router-devtools": "^1.144.0",
    "@tanstack/react-router-ssr-query": "^1.144.0",
    "@tanstack/react-start": "^1.144.0",
    "cmdk": "^1.1.1",
    "jose": "^5.9.6",
    "js-cookie": "^3.0.5",
    "markdown-it-async": "^2.2.0",
    "nitro": "^3.0.1-alpha.1",
    "octokit": "^5.0.5",
    "oxfmt": "^0.19.0",
    "react": "^18.3.1",
    "react-diff-viewer-continued": "^3.4.0",
    "react-dom": "^18.3.1",
    "shiki": "^3.20.0",
    "sst": "3.9.33",
    "vite-tsconfig-paths": "^6.0.3",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@hono/node-server": "^1.13.2",
    "@tanstack/devtools-vite": "^0.4.0",
    "@tanstack/router-plugin": "^1.144.0",
    "@types/bun": "^1.3.5",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^22.7.9",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.5.0",
    "eslint": "^9.9.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.9.0",
    "hono": "^4.6.6",
    "tsr": "^1.3.4",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.1",
    "vinxi": "^0.5.10",
    "vite": "^6.0.0",
    "vitest": "^4.0.18"
  }
}
