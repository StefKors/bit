{
  "name": "@bit/client",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev:ui": "vite",
    "dev:zero-cache": "zero-cache-dev",
    "dev:db-up": "docker compose --env-file .env -f ./docker/docker-compose.yml up",
    "dev:db-down": "docker compose --env-file .env -f ./docker/docker-compose.yml down",
    "dev:clean": "docker volume rm -f docker_zstart_pgdata && rm -rf ./zstart_replica*",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "format": "oxfmt --write ."
  },
  "dependencies": {
    "@rocicorp/zero": "^0.25.2",
    "better-auth": "^1.4.7",
    "drizzle-orm": "^0.41.0",
    "jose": "^5.9.6",
    "js-cookie": "^3.0.5",
    "oxfmt": "^0.19.0",
    "pg": "^8.16.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sst": "3.9.33",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@hono/node-server": "^1.13.2",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^22.7.9",
    "@types/pg": "^8.16.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "dotenv": "^16.4.5",
    "drizzle-kit": "^0.31.8",
    "eslint": "^9.9.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.9.0",
    "hono": "^4.6.6",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.1",
    "vercel": "^41.3.2",
    "vite": "^5.4.1"
  },
  "overrides": {
    "esbuild": "0.25.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@rocicorp/zero-sqlite3"
    ]
  },
  "trustedDependencies": [
    "@rocicorp/zero-sqlite3"
  ],
  "_": "For some reason, sst requires esbuild 0.25.0 when deploying the permission deployer"
}
